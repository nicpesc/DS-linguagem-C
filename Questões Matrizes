# Questões Matrizes
**//Questão ? **

#include <stdio.h>

int main() {
    //Declaração e inicializando de uma matriz de notas 5x4 de inteiros
   int matriz[5][4] = {
       {7, 8, 9, 4},
       {5, 10, 1, 0},
       {4, 10, 10, 10},
       {10, 1, 6, 0},
       {2, 8, 4, 0}
   };
     
   int soma = 0;
   
    //imprimindo os elementos da matriz de notas
    printf("Imprimindo a nota das quatro avaliações de cada uma das cinco matérias:\n ");
    for (int i = 0; i < 5; i++) {
        for (int j = 0; j < 4; j++) {    
            printf("%d ", matriz[i][j]);
        }
        printf("\n");
        }
   

    return 0;
}
